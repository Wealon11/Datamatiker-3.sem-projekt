<!DOCTYPE html>
<html>

<head>
    <style>
        table,
        th,
        td {

            border: 1px solid black;
        }
        table {
    border-collapse: collapse;
    width: 100%;
}
    </style>
</head>

<body>
    <h2>Temperatur tabel</h2>
    <p>Liste over temperatur og dato.</p>

    <table style="width:70%">
        <tr>
            <th>Temperatur</th>
            <th>Dato</th>
        </tr>
        <%temp.forEach(function (temperatur) { %>
            <tr>
                <td>
                    <%= temperatur.temps %>
                </td>
                <td>
                    <%= temperatur.Date %>
                </td>
            </tr>
        <% })%>
    </table>
    <p><a href="/">Navigere til konfiguration af grænseværdi for temperatur </a></p>
</body>

</html>